<h2>Smart Contract Template Browser</h2>
<div class="ui list">
    <div class="item">
        <i class="folder icon"></i>
        <div class="content">
            <div class="header">C#</div>
            <div class="description"></div>
            <div id="csharp_list" class="list">
                @foreach(var template in (Dictionary<string, string>)ViewData["CSharpTemplates"])
                {
                    <div class="item">
                        <i class="file icon"></i>
                        <div class="content">
                            <div class="header">@template.Key (@template.Value)</div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
    <div class="item">
        <i class="folder icon"></i>
        <div class="content">
            <div class="header">Python</div>
            <div class="description"></div>
            <div id="python_list" class="list">
                @foreach(var template in (Dictionary<string, string>)ViewData["PythonTemplates"])
                {
                    <div class="item">
                        <i class="file icon"></i>
                        <div class="content">
                            <div class="header">@template.Key (@template.Value)</div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
